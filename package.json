{
  "name": "js-rust-iter",
  "version": "0.0.3",
  "description": "Lazy iterators based on the Iterable protocol, inspired by Rust",
  "main": "dist/mod.cjs",
  "module": "dist/mod.js",
  "types": "dist/mod.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "test": "deno test",
    "bundle": "deno bundle mod.ts dist/mod.js",
    "types": "tsc",
    "esm-to-cjs": "babel --plugins @babel/plugin-transform-modules-commonjs dist/mod.js --out-file dist/mod.cjs",
    "build": "rm -rf dist && mkdir dist && npm run bundle && npm run esm-to-cjs && npm run types"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tmaxmax/js-rust-iter.git"
  },
  "keywords": [
    "iterator",
    "functional",
    "lazy",
    "iterable",
    "generic",
    "fp",
    "utils",
    "utilities",
    "pure",
    "rust"
  ],
  "author": "Teodor Maxim",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tmaxmax/js-rust-iter/issues"
  },
  "homepage": "https://github.com/tmaxmax/js-rust-iter#readme",
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-modules-commonjs": "^7.12.1",
    "@tsconfig/deno": "^1.0.5",
    "typescript": "^4.1.3"
  }
}
